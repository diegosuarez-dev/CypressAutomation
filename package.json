{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "Practising with this testing framework",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "cypress": "^5.6.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "cypress-iframe": "^1.0.1",
    "mochawesome": "^6.2.1"
  },
  "scripts": {
    "cyAll": "./node_modules/.bin/cypress run",
    "cyHeadedAll": "npm run cyAll -- --headed",
    "cyChromeAll": "npm run cyAll -- --browser chrome",
    "cyMochawesomeAll": "npm run cyAll -- --reporter mochawesome",
    "cyDashboardAll": "npm run cyAll -- --record --key 526c97d4-6aff-4992-bbb4-38b84c95ce40"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
